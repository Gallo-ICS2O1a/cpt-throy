gravity = .5

def setup():
    size(1200, 700)
    
def draw():
    background(255)
    
    player = Player(300, 300)
    

class Player:
    
    def __init__(self, x, y):
        self.x = x
        self.y = y
        self.big = 40
        self.velocity = 0
        
    def Update(self, gravity):
        self.velocity += gravity
        self.y += velocity 


